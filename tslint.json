{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rules": {
    "ordered-imports": false,
    "object-literal-sort-keys": false,
    "no-console": false,
    "no-bitwise": false,
    "only-arrow-functions": false,
    "no-debugger": false,
    "jsx-no-lambda": false
  },
  "linterOptions": {
    "exclude": [
      "config/**/*.js",
      "docs/**/*.js",
      "node_modules/**/*.ts"
    ]
  }
}
